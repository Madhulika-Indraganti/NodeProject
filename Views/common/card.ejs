<div class="card" style="width: 15rem;">
    <img src="<%= image %>" class="card-img-top" alt="...">
    <div class="card-body">
      <h5 class="card-title"><%= title %>
    </h5>
      <p class="card-text"><%= text %>
    </p>
    <p class="card-text">Rs. <%= price %>
    </p>
      <a href="/routes/Cart" class="btn btn-primary">Add to Cart</a>
    </div>
  </div>